/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "module": "commonjs",
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "typeRoots": ["node_modules/@types"],
    "types": ["resize-observer-browser"],
    "allowSyntheticDefaultImports": true
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ],
  "enableIvy": false,
}
